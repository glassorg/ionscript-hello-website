import "react"
    useState

export default () =>
    let [count, setCount] = useState(0)
    return
        <div/>
            "Hello Ion Parcel"
            for i in [ 1, 2, 3, 4, 5 ]
                <div key={ i }>{ i }</div>
            <button/>
                onClick: () => setCount(count + 1)
                "Click Me"
            if count > 0
                <p>Clicked {count} times</p>
